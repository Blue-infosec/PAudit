<Domains>
  <!-- Each object will be connected to using a PSSession using SSL on port 5986 -->
  <!-- Passwords are encrypted only the user account and the machine it was encrypted on can decrypt this -->
  <!-- AS DNS is required to resolve domain names each domain to enumerate can be given a DNS server to use -->
  <!-- leaving the DNS server blank will tell the script to use the current DNS serveres on the local macine. -->
  <!-- DomainName param must be a DNS hostname (FQDN) -->
  <!-- Username and Password must be encrypted using the encrypt password tool provided -->
  <Domain>
    <DomainName>ad.example.com</DomainName>
    <!-- must be an IP of a usable DNS server for the domain, required for remote domains to work -->
    <DNSServer>172.24.1.232</DNSServer>
    <Username>EXAMPLE\a-scottj</Username>
    <Password></Password>
    <!-- PS Remote session settings, by default SSL is off for PowerShell -->
    <UseSSL>0</UseSSL>
  </Domain>
</Domains>